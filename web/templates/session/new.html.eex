<h2>Login</h2>

<%= form_for @changeset, session_path(@conn, :create), fn f -> %>
  <%= label f, :email %>
  <%= email_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>

  <%= submit "Log In" %>
<% end %>
